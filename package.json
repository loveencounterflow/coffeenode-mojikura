    "coffeenode-text": "~0.1.1",
    "coffeenode-trm": "~0.1.6",
    "coffeenode-suspend": "~0.1.3",
    "coffeenode-solr": "0.0.1",
    "coffeenode-chr": "~0.1.1",
    "yatf": "~1.0.4",
    "harmony-reflect": "0.0.9",
    "asynct": "~1.1.0",
    "xpath": "0.0.5",
    "xmldom": "~0.1.16"
  },
  "keywords": [
    "database",
    "coffeescript",
    "coffeenode",
    "solr",
    "lucene"
  ],
  "author": "",
  "license": "BSD-2-Clause",
