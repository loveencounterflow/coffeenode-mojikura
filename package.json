        "lib/examples.js": 1,
        "lib/populate.js": 1,
        "lib/POSTER.js": 1,
        "lib/TIMER.js": 1,
        "lib/QUERY-tests.js": 1,
        "lib/QUERY.js": 1,
        "lib/examples/search-batch.js": 1,
        "lib/examples/examples.js": 1,
        "lib/populators/populate.js": 1,
        "lib/tests/QUERY-tests.js": 1,
        "lib/tests/tests.js": 1,
        "lib/populate/HELPERS.js": 1,
        "lib/populate/main.js": 1,
        "lib/populate/package.json": 1,
        "lib/SCHEMA.js": 1
      }
    }
  },
